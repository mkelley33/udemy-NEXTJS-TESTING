    "build": "next build",
    "build:test": "npm run db:reset && export NODE_ENV=\"test\" && next build",
    "start": "next start",
    "start:test": "export NODE_ENV=\"test\" && next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "e2e": "npm run build:test && npm run start:test",
